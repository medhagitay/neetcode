class Solution:
    def lengthOfLastWord(self, s: str) -> int:
        # l = s.split(" ")
        # return len(l[len(l)-1])
        i=len(s)-1
        count=0
        while s[i]==' ' and i>=0:
            i-=1
        print(i)
        while s[i]!=' ' and i>=0:
            count+=1
            i-=1
        return count
            
